#include <stdio.h>

int main() {
    // Define enumeration for traffic lights
    enum TrafficLight { RED, YELLOW, GREEN };

    enum TrafficLight light;
    light = GREEN;
    switch(light) {
        case RED:
            printf("Stop\n");
            break;
        case YELLOW:
            printf("Wait\n");
            break;
        case GREEN:
            printf("Go\n");
            break;
        default:
            printf("Invalid light\n");
    }

    return 0;
}
