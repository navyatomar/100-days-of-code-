Code=77
#include <stdio.h>
int main() {
    int n, m, i, j, sum = 0;
    scanf("%d %d", &n, &m);
    int matrix[n][m];
    for(i = 0; i < n; i++) {
        for(j = 0; j < m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    if(n != m) {
        printf("Not a square matrix\n");
        return 0;
    }
    for(i = 0; i < n; i++) {
        sum += matrix[i][i];
    }    printf("%d\n", sum);
    return 0;
}
Code=78
#include <stdio.h>
int main() {
    int n, m, i, j, sum = 0;
    scanf("%d %d", &n, &m);
    int matrix[n][m];
    for(i = 0; i < n; i++) {
        for(j = 0; j < m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    if(n != m) {
        printf("Not a square matrix\n");
        return 0;
    }
    for(i = 0; i < n; i++) {
        sum += matrix[i][i];
    }    printf("%d\n", sum);
    return 0;
}


