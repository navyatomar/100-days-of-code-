code=61
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int key;
    scanf("%d", &key);
    int index = -1;
    for(int i = 0; i < n; i++) {
        if(arr[i] == key) {
            index = i;
            break;
        }
    }
    if(index != -1)
        printf("Found at index %d\n", index);
    else
        printf("-1\n");
    return 0;
}
code=62
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); // Read size of array
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); // Read array elements
    }
    for(int i = 0, j = n - 1; i < j; i++, j--) {
        int temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
    }
    for(int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}

