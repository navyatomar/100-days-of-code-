
Code=99
#include <stdio.h>
#include <string.h>
int main() {
    char date[20];
    int day, month, year;
    char *months[] = {"Jan", "Feb", "Mar", "Apr", "May", "Jun",
                      "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"};
    scanf("%s", date);
    scanf(date, "%d/%d/%d", &day, &month, &year);
    printf("%02d-%s-%04d\n", day, months[month - 1], year);
    return 0;
}
Code=100
#include <stdio.h>
#include <string.h>
int main() {
    char str[100];
    int len, i, j, k;
    int first = 1;  // for proper comma placement
    scanf("%s", str);
    len = strlen(str);
    for(i = 0; i < len; i++) {
        for(j = i; j < len; j++) {
            if(!first) printf(",");  // print comma before each except the first
            for(k = i; k <= j; k++) {
                printf("%c", str[k]);
            }
            first = 0;
        }}
    printf("\n");
    return 0;
}


