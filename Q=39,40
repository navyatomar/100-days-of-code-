code=39
#include <stdio.h>

int main() {
    int n, digit, product = 1, hasOdd = 0;

    // Input number
    scanf("%d", &n);

    // Find product of odd digits
    while(n > 0) {
        digit = n % 10;        // get last digit
        if(digit % 2 != 0) {   // check if odd
            product *= digit;
            hasOdd = 1;         // mark that we found an odd digit
        }



code=40
#include <stdio.h>
#include <string.h>

int main() {
    char binary[100];
    scanf("%s", binary);

    int len = strlen(binary);

    for (int i = 0; i < len; i++) {
        if (binary[i] == '0')
            printf("1");
        else if (binary[i] == '1')
            printf("0");
        else {
            printf("\nInvalid binary number");
            return 0;
        }
    }

    return 0;
}
