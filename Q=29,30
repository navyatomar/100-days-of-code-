code=29
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long factorial = 1;  // use long long for large numbers

    for (int i = 1; i <= n; i++) {
        factorial *= i;
    }

    printf("%lld", factorial);

    return 0;
}
code=30
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int reversed = 0;
    while (n != 0) {
        int digit = n % 10;
        reversed = reversed * 10 + digit;
        n /= 10;
    }

    printf("%d", reversed);

    return 0;
}
