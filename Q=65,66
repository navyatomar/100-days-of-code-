code=65
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); // Size of the array
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); 
    }
    int key;
    scanf("%d", &key); // Element to search
    int left = 0, right = n - 1;
    int index = -1;
    while(left <= right) {
        int mid = left + (right - left) / 2; 
        if(arr[mid] == key) {
            index = mid;
            break;
        } else if(arr[mid] < key) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }
    if(index != -1)
        printf("Found at index %d\n", index);
    else
        printf("-1\n");
    return 0;
}
code=66
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); 
    int arr[n+1]; 
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); 
    }
    int key;
    scanf("%d", &key);
    int i = n - 1;
    while(i >= 0 && arr[i] > key) {
        arr[i+1] = arr[i];
        i--;
    }
    arr[i+1] = key; // Insert key
    n++; 
    for(int j = 0; j < n; j++) {
        printf("%d ", arr[j]);
    }
    printf("\n");
    return 0;
}

