#include <stdio.h>

int main() {
    int nums[] = {1, 3, 3, 4};
    int n = sizeof(nums) / sizeof(nums[0]); // array size

    int sum_actual = 0;
    for (int i = 0; i < n; i++)
        sum_actual += nums[i];
    int sum_expected = (n - 1) * n / 2;

    int repeated = sum_actual - sum_expected;
    printf("%d\n", repeated);

    return 0;
}
