code=67
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); 
    int arr[n+1]; 
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); 
    }
    int pos, key;
    scanf("%d %d", &pos, &key); 
    for(int i = n; i >= pos; i--) {
        arr[i] = arr[i-1];
    }
    arr[pos-1] = key; 
    n++; 
    for(int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
code=68
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); 
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); 
    }
    int pos;
    scanf("%d", &pos); // Position to delete (1-based)
    for(int i = pos - 1; i < n - 1; i++) {
        arr[i] = arr[i + 1];
    }n--; 
    for(int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}
