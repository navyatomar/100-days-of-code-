code= 57
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); 
    int arr[n];
    int sum = 0;
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    printf("%d\n", sum); // Print the sum
    return 0;
}
code=58
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n); // Read size of array
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); }

    int max = arr[0];
    int min = arr[0];
    for(int i = 1; i < n; i++) {
        if(arr[i] > max)
            max = arr[i];
        if(arr[i] < min)
            min = arr[i];
    }
    printf("Max=%d, Min=%d\n", max, min);
    return 0;
}
