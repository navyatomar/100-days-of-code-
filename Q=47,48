code=47
# Day_42-wap-to-print-the-following-pattern-
*
**
***
****
*****

#include<stdio.h>
int main(){
	int n=5,i,j;
	scanf("%d",&n);
	for(i=1;i<=n;i++){
		for(j=1;j<=i;j++){
			printf("*");
		}printf("\n");
	}return 0;
}
code=48
#include <stdio.h>

int main() {
    for (int i = 1; i <= 5; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", j);
        }
        printf("\n");
    }
    return 0;
}
