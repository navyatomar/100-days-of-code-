code=71
#include <stdio.h>
int main() {
    int rows, cols;
    scanf("%d %d", &rows, &cols); // Read number of rows and columns
    int matrix[rows][cols];
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }
    return 0;
}
code=72
#include <stdio.h>
int main() {
    int rows, cols;
    scanf("%d %d", &rows, &cols); // Read number of rows and columns
    int matrix[rows][cols];
    int sum = 0;
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            scanf("%d", &matrix[i][j]);
            sum += matrix[i][j];
        }
    }
    printf("%d\n", sum);
    return 0;
}

