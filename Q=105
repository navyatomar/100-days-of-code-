#include <stdio.h>

int majorityElement(int nums[], int n) {
    int candidate = -1, count = 0;

    // Step 1: Find candidate
    for (int i = 0; i < n; i++) {
        if (count == 0) {
            candidate = nums[i];
            count = 1;
        } 
        else if (nums[i] == candidate) {
            count++;
        }
        else {
            count--;
        }
    }

    // Step 2: Verify candidate
    int freq = 0;
    for (int i = 0; i < n; i++)
        if (nums[i] == candidate)
            freq++;

    if (freq > n / 2)
        return candidate;
    else
        return -1;
}

int main() {
    int nums[] = {3, 2, 3};
    int n = sizeof(nums) / sizeof(nums[0]);

    printf("%d\n", majorityElement(nums, n));

    return 0;
}
