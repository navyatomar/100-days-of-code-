93
#include <stdio.h>
#include <string.h>
int main() {
    char str1[200], str2[200];
    int freq[26] = {0};
    int i;
    scanf("%s", str1);
    scanf("%s", str2);
    if(strlen(str1) != strlen(str2)) {
        printf("Not anagrams\n");
        return 0;}
    for(i = 0; str1[i] != '\0'; i++) {
        freq[str1[i] - 'a']++;
    }    for(i = 0; str2[i] != '\0'; i++) {
        freq[str2[i] - 'a']--;
    }    for(i = 0; i < 26; i++) {
        if(freq[i] != 0) {
            printf("Not anagrams\n");
            return 0;
        }
    }  printf("Anagrams\n");
    return 0;
}
94
#include <stdio.h>
#include <string.h>
int main() {
    char str[200], word[50], longest[50];
    int i = 0, j = 0, maxLen = 0, len;
    fgets(str, sizeof(str), stdin);
    for(i = 0; i <= strlen(str); i++) {
        if(str[i] != ' ' && str[i] != '\0' && str[i] != '\n') {
            word[j++] = str[i]; // build current word
        } else {
            word[j] = '\0'; // terminate word
            len = strlen(word);
            if(len > maxLen) {
                maxLen = len;
                strcpy(longest, word);
            }
            j = 0; // reset for next word
        }
    }
    printf("%s\n", longest);
    return 0;
}

