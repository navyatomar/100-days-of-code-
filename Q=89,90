Q=89:
#include <stdio.h>
#include <string.h>
int main() {
    char str[200], ch;
    int i, count = 0;
    scanf("%s", str);
    scanf(" %c", &ch);  // note the space before %c to skip newline
    for(i = 0; str[i] != '\0'; i++) {
        if(str[i] == ch)
            count++;
    }
    printf("%d\n", count);
    return 0;
}
Q=90:
#include <stdio.h>
#include <string.h>
int main() {
    char str[200];
    int i;
    scanf("%s", str);
    for(i = 0; str[i] != '\0'; i++) {
        if(str[i] >= 'a' && str[i] <= 'z')
            str[i] = str[i] - 32;   // Convert to uppercase
        else if(str[i] >= 'A' && str[i] <= 'Z')
            str[i] = str[i] + 32;   // Convert to lowercase
    }

    printf("%s\n", str);
    return 0;
}

