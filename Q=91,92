
Code=91
#include <stdio.h>
#include <string.h>
int main() {
    char str[200], result[200];
    int i, j = 0;
    scanf("%s", str);
    for(i = 0; str[i] != '\0'; i++) {
        if(str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u' ||
           str[i]=='A' || str[i]=='E' || str[i]=='I' || str[i]=='O' || str[i]=='U')
            continue;  // skip vowels
        else
            result[j++] = str[i];
    }
    result[j] = '\0';  
    printf("%s\n", result);
    return 0;
}
Code=92
#include <stdio.h>
#include <string.h>
int main() {
    char str[200];
    int freq[26] = {0}; // for lowercase alphabets
    int i, index = -1;
    scanf("%s", str);
    for(i = 0; str[i] != '\0'; i++) {
        int pos = str[i] - 'a';
        freq[pos]++;
        if(freq[pos] == 2) { // found first repeating character
            index = i;
            break;
        }
    }
    if(index != -1)
        printf("%c\n", str[index]);
    else
        printf("No repeating character\n");
    return 0;
}


