code=7
#include <stdio.h>

int main() {
    int a, b;
    scanf("%d %d", &a, &b);

    a = a + b;
    b = a - b;
    a = a - b;

    printf("After swap: %d %d", a, b);

    return 0;
}
code=8
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int sum = n * (n + 1) / 2;

    printf("Sum=%d", sum);

    return 0;
}
