#include <stdio.h>

int main() {
    // Define enum for traffic lights
    enum TrafficLight { RED, YELLOW, GREEN };

    // Array of corresponding names
    const char *names[] = { "RED", "YELLOW", "GREEN" };

    // Loop through enum values
    for (int i = RED; i <= GREEN; i++) {
        printf("%s=%d\n", names[i], i);
    }

    return 0;
}
